{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base", "prettier"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
  	// camelcase: since GET params & database uses snake_case
		"camelcase": 0,
		// no-console: in nodeJS, console is also used to output information
		"no-console": 0
  }
}
